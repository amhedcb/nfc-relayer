generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model PaymentTx {
  uuid       String   @id
  toAddress  String
  chainId    Int
  amount     String
  contractId String
  data       Json?
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}